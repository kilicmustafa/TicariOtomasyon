
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Index</h2>


@{ 


    var grafik = new Chart(width: 500, height: 500);

    grafik.AddTitle(text: "Kategori ve Urun Sayıları");
    grafik.AddLegend(title: "Değerler");
    grafik.AddSeries(

        name: "Veriler",
        chartType: "bar",
        xValue : new [] { "Beyaz Eşya", "Bilgisayar", "Akıllı Telefon", "Akıllı Saat"},
        yValues : new [] {300 ,500, 600, 700}





        );
    grafik.Write();

}
